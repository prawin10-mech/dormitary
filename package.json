{
  "name": "dormitary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "next dev",
    "dev:wss": "dotenv -e .env tsx watch ./server/server.ts --tsconfig tsconfig.server.json",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:wss\"",
    "build": "next build",
    "build:server": "tsc",
    "lint": "next lint",
    "start": "concurrently \"next start\" \"npm run dev:wss\""
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "mongoose": "^8.4.4",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.2",
    "typescript": "^5"
  }
}
